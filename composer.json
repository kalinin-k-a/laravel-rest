{
    "name": "sanchescom/laravel-rest",
    "description": "This library provides.",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Alexander Efimov",
            "email": "sanches.com@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sanchescom\\Rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanchescom\\Rest\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-text --colors=never",
        "check-style": "./vendor/bin/phpcs -p --standard=PSR2 ./src",
        "fix-style": "./vendor/bin/phpcbf --standard=PSR2 ./src"
    },
    "require": {
        "php": "^7.2",
        "laravel/lumen-framework": "~5.8",
        "jenssegers/model": "^1.2",
        "sanchescom/json-helper": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.2",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
